<div class="formWrapper">
    <h1>Login</h1>
    <form>
        <div class="form-group">
            <label for="email">Email</label>
            <input type="email" class="form-control" id="email" name="email">
        </div>
        <div class="form-group">
            <label for="pwd">Password</label>
            <input type="password" class="form-control" id="pwd" name="pswd">
        </div>
        <div class="formbtn-Wrapper">
            <button id="btnLogin" type="button" class="btn btn-primary custombtn">Login</button>
            <div class="divider">
                <p>OR</p>
            </div>
            <a href="@Url.Action("SignUp","Account")" class="btn btn-primary custombtn">Create New Account</a>
        </div>
    </form>
</div>
@section Scripts{
    <script src="~/scripts/login/login.js"></script>
}