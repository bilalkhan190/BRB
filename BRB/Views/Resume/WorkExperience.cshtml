@model BusinessObjects.Models.WorkExperience
<div class="formWrapper">
    <div id="mainDisplayPage">
        <h1>
            Work Experience <button type="button" class="btn btn-outline-primary btn-sm"
                                    data-bs-toggle="modal" data-bs-target="#experienceModal">
                ?
            </button>
        </h1>
        <button type="button" class="mb-2 btn btn-outline-primary btn-sm" data-bs-toggle="modal"
                data-bs-target="#modalExample">
            Example
        </button>

        <p>
            Work Experience will follow with chronological listing (most recent to least) of overall work
            experience. If you had multiple positions within the same organization, the organization
            will be listed and then chronological listing of the positions held below it. Internships,
            paid or not, can be listed in the Work section of your resume. Employers expect that
            undergraduate students will not have vast amounts of experience while in school and
            internships are considered valuable for receiving practical work experience where
            transferable skills can be learned and developed.
        </p>
        @if(Model.Companies.Count == 0)
        {
            <p id="noList" class="danger-text">
                <i>You currently have no companies listed. Click the Add button below to add one.</i>
            </p>
        }
       
        <div id="companiesPenal">
           @await Html.PartialAsync("_Companies",Model.Companies)
        </div>
        <button type="button" class="btn btn-primary btn-sm custombtn w-auto mt-2" onclick="localStorage.clear();$(`input[name='CompanyId']`).val('');$('#companyForm').trigger('reset');$('#SummaryModal').modal('toggle')">
            Add a Company
        </button>

     

        <hr>
        <form class="needs-validation" novalidate>
            <div class="form-check">
                <label class="form-check-label">
                    @{
                        string isComplete = Model.IsComplete ? "checked" : "";

                    }
                    @if (Model.Companies.Count > 0 && Model.Companies.Where(x => x.Positions.Count > 0).Count() > 0)
                    {
                        <input type="checkbox" id="cbIsComplete" @isComplete
                               class="form-check-input">
                        <label for="cbIsComplete">I am finished with this section</label>
                    }
                    else
                    {
                        <input type="checkbox" id="cbIsComplete" disabled
                               class="form-check-input" >
                        <span>I am finished with this section</span>
                    }
                </label>
            </div>
            <div class="formbtn-Wrapper">
                @if (Model.Companies.Count > 0 && Model.Companies.Where(x => x.Positions.Count > 0).Count() > 0)
                {
                    <button type="button" id="btnSaveOrContinue" class="btn btn-primary custombtn w-auto mt-2">
                        Save and
                        Continue
                    </button>
                }
                else
                {
                    <button type="button" id="btnSaveOrContinue" disabled class="btn btn-primary custombtn w-auto mt-2">
                        Save and
                        Continue
                    </button>
                }

            </div>
        </form>
    </div>

</div>
